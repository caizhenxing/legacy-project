}

/* ===== 信息录入页面/信息详细页面css ====*/
table.Detail{
	border-right:1px solid #E6E6E6;
	border-left:1px solid #eee;
	border-bottom: 1px solid #eee;
	width:100%;
}
table.Detail td{
	padding: 0px 3px 0px 3px;
	border-top: 1px solid #eee;
	/*border-right: 1px solid #E6E6E6;*/
	height:20px;
	width:20%;
}
table.Detail font,table.Detail p,table.Detail span{
	word-break : break-all;
}
table.Detail td.attribute{
	padding:5px 10px 0px 0px;
	vertical-align:top;
	text-align: right;	
	height:20px;
	width:20%;
	white-space:nowrap;
	background:transparent url(../image/common/attribute_bg.gif) no-repeat right top;
	/*
	background: #F5F5F5;
	border-bottom: 1px solid #B5B8D0;
	*/
}
table.Detail td.blank{
	padding:5px 10px 0px 0px;
	vertical-align:top;
	text-align: right;	
	height:20px;
	width:20%;
	white-space:nowrap;
}
/*操作按钮所在位置的宽度*/
table td.detail_opera{
	width:6%;
	text-align:right;
	border-right: 1px solid #E6E6E6;
}

table.Detail td.detail_help{
	padding:5px 0px 0px 0px;
	vertical-align:top;	
	width:30%;
	background: #fff;
	color:#999;
}
table.Detail input,textarea{
	color:#666666;
	border:1px solid #bbbbbb;
}
.Blank{
	border:0px;
}
.Blank td{
	border:0px;
}

/*新闻使用css*/
.news_title{
	float:left;
	padding-left:5px;	
}
.news_title_bg{
	background:#F0F0F7;
	height:22px;
	border:1px solid  #666;
	padding:1px;			
}

.news_list,.news_list td{
	height:22px;
	margin-left:4px;
	margin-left:5px;
	padding-left:0px;	
	border-bottom:1px dotted  #ccc;	
	text-align:left;
	overflow:hidden;
}
.news_list_time,.news_list_time a{
	font-size:8pt;
	color:#bbb;
	text-align:right;
}
.news_list a,.news_list a:visited,.news_list a:hover{
	text-decoration: none;
}
.news_more{
	padding:5px 5px 5px 5px;
	text-align:right;
}

/* 文字效果css */
.font_master,.font_master a,.font_master:hover,.font_master:visited,.font_master:active{
	color:#3300ff;
	font-weight:bold;
}
.font_keyword{
	color:red;
}
.font_request{
	color:red;
}
.font_error{
	height:18px;
	background-image:url(../image/main/warn.gif);
	background-repeat:no-repeat;
	padding-left:20px;	
	color:red;
}
.font_ignore {
	color: #bbb;
}
.font_blur{
	color:#888888;
}
.font_help b{
	font-weight : bold;
}
.font_help{
	background-image:url(../image/common/help_list.gif);
	background-repeat:no-repeat;
	padding-left:16px;
	margin-top:5px;
	color:#999;
}
.font_help ul{
	margin:5px 0px 2px 0px;
	padding-left:24px !important;
	padding-left:16px;
}
.font_help li{
	border-bottom:1px dotted #e7e7e7;
	line-height:18px;
	width:100%
}

.font_link,a .font_link{
	background-image:url(../image/common/help_link.gif);
	background-repeat:no-repeat;
	white-space:nowrap;
	cursor:pointer;
	padding-left:15px;
	text-decoration: underline; 
}
.font_note{
	background-image:url(../image/common/help_note.gif);
	background-repeat:no-repeat;
	padding-left:15px;
	color:#FF6600;
}
.font_unit{
	color:#999999;
	font-size:8pt;
}
.font_title{
	padding-left:2px;
	line-height:18px;
}
.font_title u{
	background-image:url(../image/common/li.gif);
	background-repeat:no-repeat;
	margin-left :5px;
	padding-left:20px;
}
/*金钱表示*/
.font_money{
	text-align:right;
}
.font_yes{
	color:green;
}
.font_no{
	color:#FF9900;
}


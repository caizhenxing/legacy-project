  <action-mappings>
  
  
<!-- 公司基本信息的action -->
     <action
      attribute="workUnitInfoBean"
      name="workUnitInfoBean"
      path="/sys/staff/workUnitInfo"
      parameter="method"
      scope="request"
      type="org.springframework.web.struts.DelegatingActionProxy" >
           <forward name="main"  path="/sys/staff/workUnitInfo/Main.jsp"/>
           <forward name="popquery" path="/sys/staff/workUnitInfo/popWorkUnitInfo.jsp"/>
		   <forward name="toWorkUnitInfoList" path="/sys/staff/workUnitInfo/workUnitInfoList.jsp"/>
           <forward name="toWorkUnitInfoLoad"  path="/sys/staff/workUnitInfo/workUnitInfoLoad.jsp"/>
           <forward name="workUnitInfoMain"  path="/sys/staff/workUnitInfo/workUnitInfoMain.jsp"/>
           <forward name="topQuery" path="/sys/staff/workUnitInfo/topQuery.jsp"/>
 
    </action>
    
    
    
    <!-- 员工基本信息的action -->
         <action
      attribute="staffBasicBean"
      name="staffBasicBean"
      path="/sys/staff/staffBasic"
      parameter="method"
      scope="request"
      type="org.springframework.web.struts.DelegatingActionProxy" >
          
           
           <forward name="toStaffBasicQuery" path="/sys/staff/staffBasic/staffBasicQuery.jsp"/>

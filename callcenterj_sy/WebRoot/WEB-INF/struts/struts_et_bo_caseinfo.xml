		<action attribute="generalCaseinfoBean"
			name="generalCaseinfoBean" scope="request"
			path="/caseinfo/generalCaseinfo"
			type="org.springframework.web.struts.DelegatingActionProxy"
			parameter="method">

			<forward name="toGeneralCaseinfoMain"
				path="/caseinfo/generalCaseinfo/generalCaseinfoMain.jsp" />
			<forward name="toGeneralCaseinfoList"
				path="/caseinfo/generalCaseinfo/generalCaseinfolist.jsp" />
			<forward name="toGeneralCaseinfoLoad"
				path="/caseinfo/generalCaseinfo/generalCaseinfoLoad.jsp" />
			<forward name="toGeneralCaseinfoQuery"
				path="/caseinfo/generalCaseinfo/generalCaseinfoQuery.jsp" />
			<forward name="upload"
				path="/caseinfo/generalCaseinfo/generaUpload.jsp" />
			<forward name="uploadVideo"
				path="/caseinfo/generalCaseinfo/generaUploadVideo.jsp" />
			<forward name="toGeneralCaseinfoStatisticQuery"
				path="/caseinfo/generalCaseinfo/generalCaseinfoStatisticQuery.jsp" />
      <forward name="main1" path="/caseinfo/generalCaseinfo/generalCaseinfoMain1.jsp" />

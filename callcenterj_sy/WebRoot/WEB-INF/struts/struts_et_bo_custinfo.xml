	<global-exceptions />
	<global-forwards />
	<action-mappings>
		<!-- 客户action -->
		<action attribute="custinfo" 
				name="custinfo" 
				scope="request" 
				path="/custinfo/custinfo" 
				type="org.springframework.web.struts.DelegatingActionProxy" 
				parameter="method">
			<forward name="main"  path="/custinfo/custinfoMain.jsp"/>
			<forward name="list"  path="/custinfo/custinfoList.jsp"/>
			<forward name="load"  path="/custinfo/custinfoLoad.jsp"/>
			<forward name="query" path="/custinfo/custinfoQuery.jsp"/>
			<forward name="alllist"  path="/custinfo/custinfoAll.jsp"/>
			<forward name="allExpertList"  path="/custinfo/custinfoExpertAll.jsp"/>
			<forward name="phonemain"  path="/custinfo/phone/custPhoneMain.jsp"/>
			<forward name="phonelist"  path="/custinfo/phone/custPhoneList.jsp"/>
			<forward name="phoneload"  path="/custinfo/phone/custPhoneLoad.jsp"/>
			<forward name="phonequery" path="/custinfo/phone/custPhoneQuery.jsp"/>
		</action>
		
		
		<!-- 通讯录action -->
		<action attribute="phoneinfo" 
				name="phoneinfo" 
				scope="request" 
				path="/custinfo/phoneinfo" 
				type="org.springframework.web.struts.DelegatingActionProxy" 
				parameter="method">
			<forward name="main"  path="/custinfo/phoneinfo/custPhoneMain.jsp"/>
			<forward name="query" path="/custinfo/phoneinfo/custPhoneQuery.jsp"/>
			<forward name="list"  path="/custinfo/phoneinfo/custPhoneList.jsp"/>
			
			<forward name="custload"  path="/custinfo/phoneinfo/custPhoneLoad.jsp"/>
			<forward name="agentload"  path="/custinfo/phoneinfo/agentLoad.jsp"/>
			<forward name="expertload"  path="/custinfo/phoneinfo/expertLoad.jsp"/>
			<forward name="projectload"  path="/custinfo/phoneinfo/projectLoad.jsp"/>
			<forward name="mediaload"  path="/custinfo/phoneinfo/mediaLoad.jsp"/>
			<forward name="govload"  path="/custinfo/phoneinfo/govLoad.jsp"/>
			<forward name="connectload"  path="/custinfo/phoneinfo/connectLoad.jsp"/>
			<forward name="commonload"  path="/custinfo/phoneinfo/commonLoad.jsp"/>
			<forward name="countinsert"  path="/custinfo/phoneinfo/custPhoneAdd.jsp"/>
			<forward name="error"  path="/custinfo/phoneinfo/error.jsp"/>
			
			<forward name="linkmain"  path="/custinfo/phoneinfo/custLinkManMain.jsp"/>
			<forward name="linkquery" path="/custinfo/phoneinfo/custLinkManQuery.jsp"/>
			<forward name="linklist"  path="/custinfo/phoneinfo/custLinkManList.jsp"/>
			<forward name="linkmanload"  path="/custinfo/phoneinfo/linkmanLoad.jsp"/>
		</action>
		
		<!-- 来电弹出窗口action -->
		<action attribute="openwin" 
				name="openwin" 
				scope="request" 
				path="/custinfo/openwin" 
				type="org.springframework.web.struts.DelegatingActionProxy" 
				parameter="method">

			<forward name="load"  path="/custinfo/openwin.jsp"/>
			<forward name="questionList" path="/custinfo/history.jsp"/>
			<forward name="search" path="/custinfo/search.jsp"/>
		</action>


	</action-mappings>
	
</struts-config>


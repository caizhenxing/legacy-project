<!--
  // 「評価入力用ファイルフォーマットダウンロード」をクリック
  function onDownloadFile(jigyoId, downloadFileFlg){
      document.download_form.action="$link.setAction('/shinsa/shinsaJigyo/formatHyokaFileDownload')";
      document.download_form.downloadFileFlg.value = downloadFileFlg;
      document.download_form.jigyoId.value = jigyoId;
      document.download_form.submit();
  }

  // 「登録済みファイルダウンロード」をクリック
  function onRegDownloadFile(systemNo, shinsainNo, jigyoKubun){
      document.regdownload_form.action="$link.setAction('/shinsa/shinsaJigyo/regHyokaFileDownload')";
      document.regdownload_form.systemNo.value = systemNo;
      document.regdownload_form.shinsainNo.value = shinsainNo;
      document.regdownload_form.jigyoKubun.value = jigyoKubun;
      document.regdownload_form.submit();
  }

  // 「保存」をクリック
  function onAddClick(jigyoCd){
      /* 総合評点で選択されているラジオボタンの値を取得 */
      kekkaTenValue = '';
      for(i = 0; i < document.add_form.kekkaTen.length; i ++){ 
          if(document.add_form.kekkaTen[i].checked){ 
              kekkaTenValue = document.add_form.kekkaTen[i].value;
          }
      }

      /* 総合評点が「5」または「1」でコメントが未入力の場合 */
//2006/10/30 楊　修正ここから
      //if((document.add_form.rigai.checked == false) && (kekkaTenValue == '5' || kekkaTenValue == '1')  && (document.add_form.comments.value == null || document.add_form.comments.value == '')){
	  if((kekkaTenValue == '5' || kekkaTenValue == '1')  && (document.add_form.comments.value == null || document.add_form.comments.value == '')){
//2006/10/30　楊　修正ここまで   
          if(confirm("総合評点が「５」又は、「１」で、審査意見が入力されていませんがよろしいですか？")){
              document.add_form.action="$link.setAction('/shinsa/shinsaJigyo/shinsaKekkaInputSave')";
              document.add_form.submit();
          }
      /* それ以外 */
      }else{
          document.add_form.action="$link.setAction('/shinsa/shinsaJigyo/shinsaKekkaInputSave')";
          document.add_form.submit();
      } 
  }
//-->
</script>

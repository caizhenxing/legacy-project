    <!-- ### ご意見ご要望入力画面 ### -->
    <action name="ikenForm" path="/iken" scope="session" 
            type="jp.go.jsps.kaken.web.struts.ForwardAction" unknown="false"
            validate="false">
      <forward name="success" path="/pages/iken/iken.vm" redirect="false"/>
    </action>
    <!-- ### ご意見ご要望入力確認画面 ### -->
    <action name="ikenForm" path="/ikenKakunin" scope="session"

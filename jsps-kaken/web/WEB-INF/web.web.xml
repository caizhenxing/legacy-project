      <url-pattern>/shozoku/pages/*</url-pattern>
      <url-pattern>/shinsa/pages/*</url-pattern>
      <url-pattern>/gyomu/pages/*</url-pattern>
      <url-pattern>/system/pages/*</url-pattern>
      <url-pattern>/errors/*</url-pattern>
    </web-resource-collection>

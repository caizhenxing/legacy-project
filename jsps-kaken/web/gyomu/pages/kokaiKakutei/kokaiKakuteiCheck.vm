#errorMarkup()
<form method="post" action="$link.setAction('/gyomu/kokaiKakutei/kokaiKakuteiSave')">
   ## A hidden form field with the transaction token
   <input type="hidden" name="$form.tokenName" value="$form.token">
   ##



<!-- 一覧用テーブルここから -->
<table width="100%" border="0" cellpadding="0" cellspacing="0" class="tableBackg">
 <tr>
  <td>
   <table width="100%" border="0" cellpadding="3" cellspacing="1" class="table1">
    <tr>
     <th>研究種目名</th>
     <th>業務担当課</th>
     <th>学振受付期間</th>
    </tr>
    #if($list.size() == 0)
		##該当件数が０件だった場合
	    <tr>
	    	<td>&nbsp;</td>
	    	<td>&nbsp;</td>
	    	<td>&nbsp;</td>
	    	<td>&nbsp;</td>
	    	<td>&nbsp;</td>
	    	<td>&nbsp;</td>
	    </tr>
    #else	
	  #foreach($user in $list)
      #set( $count = $velocityCount - 1)
	    <tr>
	     <td>
	     	<input type="hidden" name="jigyoId[$count]" value="$!escape.getText($user.JIGYO_ID)">	     	
	     	#if($user.KAISU.toString() == '1')
	     		平成$!escape.getText($!user.NENDO)年度 $!escape.getText($!user.JIGYO_NAME)
	     	#else
	     		平成$!escape.getText($!user.NENDO)年度 第$!escape.getText($!user.KAISU)回 $!escape.getText($!user.JIGYO_NAME)
			#end
		 </td>
	     <td>$!escape.getText($!user.TANTOKA_NAME)</td>
	     <td>$!dateFormat.format($!escape.getText($!user.UKETUKEKIKAN_START))〜$!dateFormat.format($!escape.getText($!user.UKETUKEKIKAN_END))</td>
	    </tr>
	  #end
	#end
   </table>
  </td>
 </tr>

#parse( $commonSetting )
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=Shift_JIS">
<title>JSPS 電子申請システム</title>
<link href="$path/css/jspsstyle.css" rel="stylesheet" type="text/css">
<script type="text/javascript" language="javascript" src="$path/js/fncWindowOpen.js"></script>
</head>
<body class="bodyStyle">
<div align="center">
 <!-- ヘッダー 用テーブルここから-->
 <table width="95%" border="0" cellpadding="0" cellspacing="0">
  <tr>
   <td width="307" valign="top" background="$path/images/header_bg2.gif"><img src="$path/images/header_logo.gif" alt="JSPS　独立行政法人　日本学術振興会　電子申請システム" width="307" height="58"></td>
   <td align="right" background="$path/images/header_bg2.gif">
    <!-- ヘルプ、ログアウトボタン用テーブルここから -->
    <table border="0" cellpadding="0" cellspacing="1">
     <tr>
      <td colspan="2"><img src="$path/images/spacer.gif" alt="" width="1" height="15"></td>
     </tr>
     <tr>
      <td><a href="javascript:fncWindowOpen('$path/docs/manual3.pdf','help');"><img src="$path/images/header_help.gif" alt="ヘルプ" width="72" height="20" border="0"></a></td>
      <td><a href="$link.setAction('/gyomu/logoff')"><IMG SRC="$path/images/header_logout.gif" ALT="ログアウト" WIDTH=72 HEIGHT=20 border="0"></a></td>
     </tr>
    </table>
    <!-- ヘルプ、ログアウトボタン用テーブルここまで -->
   </td>
   <td width="6" align="right" background="$path/images/header_bg2.gif"><img src="$path/images/header_bg3.gif" alt="" width="6" height="58"></td>
  </tr>
 </table>
 <!--  ヘッダー 用テーブルここまで-->
 <!-- 左右の縦線用テーブルここから -->
 <table width="95%" border="0" cellpadding="0" cellspacing="0">
  <tr>
   <td width="1" bgcolor="#CCCCCC"><img src="$path/images/spacer.gif" alt="" width="1" height="1"></td>
   <td>
    <!-- コンテンツエリアここから -->
    <!-- ログインユーザ、ルート情報用テーブルここから　-->
    <table width="100%" border="0" cellpadding="4" cellspacing="0">
     <tr>
      <td bgcolor="#999966" class="rootFont"><font color="#FFFFFF"><a href="$link.setAction('/gyomu/showMainMenu')">業務担当者向けメニュー</a>＞<b>チェックリスト検索</b><font size="1">＞チェックリスト一覧</font></font></td>
     </tr>
    </table>
    <!-- ログインユーザ、ルート情報用テーブルここまで　-->
    <!-- サブ画面タイトル用テーブルここから -->
    <table width="100%" border="0" cellpadding="0" cellspacing="0">
     <tr valign="bottom">
      <td width="1%" background="$path/images/subtitle_img2.gif"><IMG SRC="$path/images/subtitle_img1.gif" WIDTH=61 HEIGHT=49 ALT=""></td>
      <td width="90%" valign="middle" background="$path/images/subtitle_img2.gif" class="subTitlefont">
       <br>チェックリスト検索</td>
      <td width="1%" align="right" background="$path/images/subtitle_img2.gif"> <IMG SRC="$path/images/subtitle_img3.gif" WIDTH=213 HEIGHT=49 ALT=""></td>
     </tr>
    </table>
    <!-- サブ画面タイトル用テーブルここまで -->

    <!-- 本文ここから -->
    <p align="left">チェックリストの検索を行います。</p>
    <p align="left">検索条件を入力して[検索実行]ボタンをクリックしてください。</p>
    #errorMarkup()
    <form method="POST" action="$link.setAction('/gyomu/checkList/searchList')">
     <input type="hidden" name="startPosition" value="0">

     <!-- 一覧用テーブルここから -->
     <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tableBackg">
      <tr>
       <td>
        <table width="100%" border="0" cellpadding="3" cellspacing="1" class="table1">
         <tr>
          <th width="25%" align="left">研究種目名</th>
          <td width="75%">
	        #if($checkListSearchForm.jigyoKbn=="4"||$checkListSearchForm.jigyoKbn=="7")
	          <select name= "jigyoCd">
	          <option value="">　</option>
	          #showOptions($checkListSearchForm.jigyoList $checkListSearchForm.jigyoCd)
	          </select>[完全一致]
	        #else 若手研究(スタートアップ)
	        #end
          </td>
         </tr>
<!--     <tr>
          <th align="left">所属研究機関番号</th>
          <td><input size="5" name="shozokuCd" value="$!escape.getText($!checkListSearchForm.shozokuCd)">　<a href="$link.setAction('/kikanList')" target="_blank"><img src="$path/images/bt_list.gif" width=41 height=22 alt="一覧" border="0"></a>[完全一致]</td> 
         </tr>
-->    
         <tr>
          <th align="left" rowspan="2">所属研究機関名</th>
          <td>（番号）<input size="8" name="shozokuCd" value="$!escape.getText($!checkListSearchForm.shozokuCd)">　<a href="$link.setAction('/kikanList')" target="_blank"><img src="$path/images/bt_list.gif" width=41 height=22 alt="一覧" border="0"></a>[完全一致]</td> 
         </tr>
         <tr>
          <td>（名称）<input size="40" name="shozokuName" value="$!escape.getText($!checkListSearchForm.shozokuName)">
			[部分一致]</td>  
         </tr>
         <tr>
          <th align="left">受理状況</th>
	      <td>#showRadios("juriJokyo" $checkListSearchForm.juriList $checkListSearchForm.juriJokyo) 
	      </td>
         </tr>
        </table>
       </td>
      </tr>
     </table>
     <!-- 一覧用テーブルここまで-->
     <br>
     <!-- 本文ここまで -->

     <!-- ボタン用テーブルここから --> 
     <div align="center">
      <table border="0" cellpadding="0" cellspacing="4">
       <tr>
        <td><input type="image" src="$path/images/bt_search.gif" name="submit" width=114 height=28 border="0" alt="検索実行"></td>
        <td><a href="$link.setAction('/gyomu/showMainMenu')"><img src="$path/images/bt_bk_menu.gif" width=114 height=28 border="0" alt="メニューに戻る"></a></td>
       </tr>
      </table>
     </div>
     <!-- ボタン用テーブルここまで -->
     <!-- サブ画面フッター用テーブルここから -->
     <table width="100%" border="0" cellpadding="0" cellspacing="0">
      <tr bgcolor="#336699">
       <td width="8" background="$path/images/footer_img2.gif"> <IMG SRC="$path/images/footer_img1.gif" WIDTH=8 HEIGHT=26 ALT=""></td>
       <td align="right" background="$path/images/footer_img2.gif"><a href="$link.setAction('/gyomu/logoff')"><IMG SRC="$path/images/footer_logout.gif" ALT="ログアウト" WIDTH=72 HEIGHT=26 border="0"></a></td>
       <td width="8" align="right" background="$path/images/footer_img2.gif"> <IMG SRC="$path/images/footer_img3.gif" WIDTH=8 HEIGHT=26 ALT=""></td>
      </tr>
     </table>
    </form>
    <!-- サブ画面フッター用テーブルここまで -->
    <!-- コンテンツエリアここまで -->
   </td>
   <td width="1" bgcolor="#CCCCCC"><img src="$path/images/spacer.gif" alt="" width="1" height="1"></td>
  </tr>
 </table>
 <!-- 左右の縦線用テーブルここまで -->
</div>
</body>
</html>
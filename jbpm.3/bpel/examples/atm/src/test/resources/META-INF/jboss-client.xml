<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE jboss-client PUBLIC "-//JBoss//DTD Application Client 4.0//EN"
   "http://www.jboss.org/j2ee/dtd/jboss-client_4_0.dtd">
<jboss-client>
  
  <!-- JNDI name of client environment context -->	
  <jndi-name>atm-client</jndi-name>
  
  <service-ref>
	  <!-- service-ref element to configure -->
	  <service-ref-name>service/ATM</service-ref-name>
	  <!-- published WSDL document -->
	  <wsdl-override>http://localhost:8080/atm/frontEnd?wsdl</wsdl-override>
  </service-ref>
	
</jboss-client>